{
  "swagger": "2.0",
  "info": {
    "title": "Wordplay-go API."
  },
  "paths": {
    "/me": {
      "get": {
        "tags": [
          "users"
        ],
        "operationId": "GetUser",
        "responses": {
          "200": {
            "$ref": "#/responses/userResponse"
          }
        }
      }
    },
    "/users": {
      "post": {
        "tags": [
          "users"
        ],
        "operationId": "createUser",
        "parameters": [
          {
            "type": "string",
            "name": "Username",
            "in": "query"
          },
          {
            "type": "string",
            "name": "Password",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/userResponse"
          }
        }
      }
    }
  },
  "responses": {
    "userResponse": {
      "headers": {
        "Email": {
          "type": "string"
        },
        "ID": {
          "type": "integer",
          "format": "int64"
        },
        "Name": {
          "type": "string"
        },
        "Username": {
          "type": "string"
        }
      }
    }
  }
}